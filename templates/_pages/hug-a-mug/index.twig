{% extends '_layouts/base.twig' %}

{% block head %}
	<link rel="stylesheet" href="https://unpkg.com/swiper@11/swiper-bundle.min.css"/>
{% endblock %}

{% block body %}

	{% include '_components/video_landing.twig' with { src: '/assets/videos/placeholder.mov' } %}

	<section class="w-full flex flex-col justify-center items-center relative overflow-hidden p-8 md:p-16 min-h-screen sm:min-h-fit gap-y-16 md:gap-y-48">

		{# Hug A Mug Intro Section #}
		{% include '_components/sections/image_text_section.twig' with {
            layout: 'img_right_text_left',
            image_src: '/assets/images/quirky-emporium.png',
            image_alt: 'Quirky Emporium',
            title: 'Hug a Mug',
            text: 'Hug A Mug Ability Café, located in Castle Mall, Antrim, is a social enterprise operated by The Bridge Association, a not-for-profit organisation dedicated to providing accredited training and employment opportunities for individuals with disabilities. Opened in June 2024, this 30-seater café offers a welcoming environment where patrons can enjoy a variety of hot drinks, cakes, breakfasts, and lunches. Beyond its culinary offerings, Hug A Mug serves as a vital community hub, fostering social connections and supporting initiatives like the Antrim and Newtownabbey Loneliness Network\'s Chatty Café scheme, aimed at combating loneliness and social isolation.',
            image_aos_delay: '300',
            text_aos_delay: '100',
            extra_class: 'md:min-h-screen',
        } %}
		

        {# Cafe Opening Times & Map Section #}
        {% include '_components/sections/cafe_times_map_section.twig' with {
            logo_src: '/assets/images/quirky-emporium/quirky-emporium.svg',
            logo_alt: 'Quirky Emporium Logo',
            marker_src: '/assets/images/quirky-emporium/marker.svg',
            map_iframe_src: 'https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2312.9919656190687!2d-6.029954107774841!3d54.56889330219826!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x4861066098ac4325%3A0x6ad657a69fb1fe1f!2s33%20Colinbrook%20Dr%2C%20Dunmurry%2C%20Belfast%20BT17%200PG%2C%20UK!5e0!3m2!1sen!2sus!4v1748430186776!5m2!1sen!2sus',
            aos_delay_left_block: '400',
            aos_delay_right_block: '300'
        } %}

        {# Define your menu data for the cafe_menu_section component #}
        {% set menu_left_column = [
            {
                title: 'Bagel\'s',
                items: [
                    { name: 'Smoked Bacon & Cream Cheese with side salad/dressing', price: '£4.00' },
                    { name: 'Cream Cheese & Chive', price: '£3.50' },
                    { name: 'Tuna Melt', price: '£4.20' }
                ]
            },
            {
                title: 'Panini\'s',
                items: [
                    { name: 'Chicken & Pesto', price: '£4.50' },
                    { name: 'Ham & Cheese', price: '£4.00' },
                    { name: 'Vegetable & Hummus', price: '£4.30' }
                ]
            },
            {
                title: 'Pizza Slices',
                items: [
                    { name: 'Pepperoni', price: '£3.00' },
                    { name: 'Mushroom & Olive', price: '£2.80' },
                    { name: 'Chicken & Sweetcorn', price: '£3.10' }
                ]
            },
            {
                title: 'Toasties',
                items: [
                    { name: 'Cheese & Onion', price: '£3.20' },
                    { name: 'Bacon & Brie', price: '£3.80' },
                    { name: 'Tomato & Mozzarella', price: '£3.50' }
                ]
            },
            {
                title: 'Sandwiches',
                items: [
                    { name: 'BLT', price: '£3.70' },
                    { name: 'Egg Mayonnaise', price: '£3.00' },
                    { name: 'Club Sandwich', price: '£4.80' }
                ]
            }
        ] %}

        {% set menu_right_column = [
            {
                title: 'Other items',
                items: [
                    { name: 'Soup of the Day', price: '£3.00' },
                    { name: 'Side Salad', price: '£2.00' },
                    { name: 'Chips', price: '£2.50' }
                ]
            },
            {
                title: 'Children\'s Menu',
                items: [
                    { name: 'Mini Pizza', price: '£2.50' },
                    { name: 'Kids Sandwich Box', price: '£3.00' },
                    { name: 'Chicken Nuggets & Chips', price: '£4.00' }
                ]
            },
            {
                title: 'Weekly Special Offer',
                items: [
                    { name: 'Special Item 1', price: '£6.00' },
                    { name: 'Special Item 2', price: '£5.50' },
                    { name: 'Special Item 3', price: '£7.00' }
                ]
            },
            {
                title: 'Ice Cream',
                items: [
                    { name: 'Vanilla Scoop', price: '£1.50' },
                    { name: 'Chocolate Sundae', price: '£3.00' },
                    { name: 'Strawberry Cone', price: '£2.00' }
                ]
            },
            {
                title: 'Hot Drinks',
                items: [
                    { name: 'Coffee', price: '£2.00' },
                    { name: 'Tea', price: '£1.80' },
                    { name: 'Hot Chocolate', price: '£2.50' }
                ]
            }
        ] %}

		{# Include the Cafe Menu Component #}
		{% include '_components/sections/cafe_menu_section.twig' with {
            aos_delay: '200',
            left_column_data: menu_left_column,
            right_column_data: menu_right_column
        } %}

	</section>

	{# Section for the brick images #}
	<section class="w-full my-32 flex flex-col items-center justify-center relative overflow-hidden">
		{% include '_components/sections/brick_images_section.twig' with {
            image1_src: 'https://www.allrecipes.com/thmb/ElKonA1CRuvBP36EunGE4GVkfJo=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/AR-6761-tasty-buns-DDMFS-4x3-87727702d7944a07897f59ba6d3ab21d.jpg',
            image2_src: 'https://upload.wikimedia.org/wikipedia/commons/1/10/Sesame_seed_hamburger_buns.jpg',
            image3_src: 'data:image/jpeg;base64,',
            aos_delay1: '500',
            aos_delay2: '1000',
            aos_delay3: '1500'
        } %}
	</section>

{% endblock %}
